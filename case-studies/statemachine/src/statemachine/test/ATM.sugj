package statemachine.test;

import statemachine.Metamodel;

public statemachine ATM {
  initial state Init 

  events DoWithdraw, Cancel, PinOK, PinNOK, MoneyTaken, CardTaken, CardRevoked

  state Init {
    DoWithdraw => Withdraw
    Cancel => Init
  }
  state Withdraw {
    PinOK => GiveMoney
    PinNOK => RevokeCard
    Cancel => Init
  }
  state GiveMoney {
    MoneyTaken => ReturnCard
  }
  state ReturnCard {
    CardTaken => Init
  }
  state RevokeCard {
    CardRevoked => Init
  }
}